    <h3>Student List</h3>
  </div>
  <div class="modal-body">
    <table class="table">
      <thead>
      <tr>
        <td><input type="checkbox" class="check-all-emails" data-target=".checkbox-emails" checked/></td>
        <td>Name</td>
        <td>Email</td>
      </tr>
      </thead>
      <tbody id="emails-tbody">
      </tbody>
    </table>
  </div>
  <div class="modal-footer">
    <a href="#" class="btn" data-dismiss="modal">Close</a>
